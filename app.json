      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.socialEpoque.socialEp",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.266866452601-tf50lile57306ub9asv445pg6i6f1uke"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.socialEpoque.socialEp",
      "googleServicesFile": "./google-services.json"
    }
  }
}
